{#- -*- mode: jinja2 -*- -#}
<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for url, lastmod in urls %}
  <url>
    <loc>{{ url }}</loc>
    {% if lastmod %}
      <lastmod>{{ lastmod.isoformat() }}</lastmod>
    {% endif %}
  </url>
  {% endfor %}
</urlset>
